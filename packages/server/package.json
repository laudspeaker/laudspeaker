{
  "name": "laudspeaker-api",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "env-cmd -f .env npm run prebuild && env-cmd -f .env nest start --watch",
    "start:dev:db": "./src/common/scripts/start-db.sh",
    "start:debug": "npm run prebuild && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "env-cmd -f src/common/envs/development.env jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd -f src/common/envs/development.env jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build && typeorm-ts-node-commonjs -d ./src/data-source.ts",
    "typeorm:generate": "npm run typeorm migration:generate",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@depyronick/nestjs-clickhouse": "^1.0.16",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^8.0.3",
    "@slack/oauth": "^2.5.4",
    "@slack/web-api": "^6.7.2",
    "@types/bcryptjs": "^2.4.2",
    "async-dash": "^1.0.4",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.8.4",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "env-cmd": "^10.1.0",
    "form-data": "^4.0.0",
    "liquidjs": "^9.42.0",
    "lodash": "^4.17.21",
    "mailgun.js": "^7.0.4",
    "mongoose": "^6.4.7",
    "nest-winston": "^1.6.2",
    "nestjs-mailgun": "^3.1.3",
    "passport": "^0.5.3",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "posthog-node": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tr-nestjs-clickhouse": "^1.1.1",
    "tst-reflect": "^0.12.0",
    "typeorm": "^0.3.7",
    "winston": "^3.8.1",
    "winston-papertrail": "^1.0.5",
    "winston-syslog": "^2.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.7",
    "@types/bull": "^3.15.8",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.26",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.2.5",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "tst-reflect-transformer": "^0.12.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

